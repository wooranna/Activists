verlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type !== 'color'\">\n    <!-- End block parameters -->  \n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    \n    <div class=\"animation__background\" mbr-if=\"showAnimation\">\n        <div class=\"circle1\"></div>\n        <div class=\"circle2\"></div>\n        <div class=\"circle3\"></div>\n        <div class=\"circle4\"></div>\n        <img src=\"../_images/figure/quadr.svg\" class=\"quadr\" alt>\n    </div>\n    \n    <div class=\"container\">\n        <div class=\"row\">\n            <div class=\"col-lg-5 col-md-6 col-12\">\n                <div class=\"title__block\">\n                    <h4 class=\"mbr-section-subtitle mbr-medium mbr-fonts-style\" mbr-if=\"showSubtitle\" mbr-theme-style=\"display-7\" data-app-selector=\".mbr-section-subtitle\">PRICING PLAN</h4>\n                    <h3 class=\"mbr-section-title mbr-medium mbr-fonts-style\" mbr-if=\"showTitle\" mbr-theme-style=\"display-2\" data-app-selector=\".mbr-section-title\">Pricing Plan</h3>\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-if=\"showText\" mbr-theme-style=\"display-4\" data-app-selector=\".mbr-text\">Lorem ipsum dolor sit amet, consectetur adipisicing elit. <br>Odit sed temporibus voluptatum minus ipsa blanditiis doloremque.</p>\n                  \n                </div>\n                <ul class=\"nav nav-tabs\" role=\"tablist\">\n                    <li mbr-buttons mbr-theme-style=\"display-7\" class=\"nav-item first mbr-section-btn\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrBtnColor,-mbrLink,-mbrBtnRemove\"><a class=\"btn mbr-fonts-style btn-sm show btn-danger active\" role=\"tab\" data-toggle=\"tab\" href=\"#tabs1-5_tab0\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">SINGLE VISIT</a></li>\n                    <li mbr-buttons mbr-theme-style=\"display-7\" class=\"nav-item mbr-section-btn\" data-toolbar=\"-mbrBtnAdd,-mbrBtnMove,-mbrBtnColor,-mbrLink,-mbrBtnRemove\"><a class=\"btn mbr-fonts-style btn-sm show btn-danger active\" role=\"tab\" data-toggle=\"tab\" href=\"#tabs1-5_tab1\" data-app-placeholder=\"Type Text\" aria-selected=\"true\">MONTHLY VISIT</a></li>\n                </ul>\n            </div>\n            <div class=\"col-lg-6 col-md-6 col-12 ml-auto\">\n        <div class=\"tab-content\">\n            \n            <div id=\"tab1\" class=\"tab-pane\" role=\"tabpanel\">\n            <div class=\"plan justify-content-center\">\n                <div class=\"plan-header text-center\">\n                    <h4 class=\"plan-title mbr-medium mbr-fonts-style\" mbr-if=\"showCardTitle\" mbr-theme-style=\"display-7\" data-app-selector=\".plan-title\">\n                        SINGLE VISIT</h4>\n                    <div class=\"plan-price\">\n                        <h5 mbr-text class=\"price-figure mbr-medium mbr-fonts-style\" mbr-theme-style=\"display-1\" data-app-selector=\".price-figure\" data-toolbar=\"-mbrColor\">$10</h5>\n                        <h6 mbr-text class=\"price-term mbr-fonts-style\" mbr-if=\"showCardTerm\" mbr-theme-style=\"display-5\" data-app-selector=\".price-term\">Per month</h6>\n                    </div>\n                </div>\n                <div class=\"plan-body\">\n                    <div class=\"plan-list align-center\">\n                        <ul mbr-text class=\"mbr-list mbr-fonts-style\" mbr-theme-style=\"display-7\">\n                            <li class=\"list__item\">Preparation for School\n</li><li class=\"list__item\">Speech Therapist\n</li><li class=\"list__item\">Creative Workshop\n